  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-jest": "^9.0.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "highlight.js": "^8.7.0",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.8.1",
    "jest-cli": "^0.9.1",
    "jsx-to-string": "^0.2.10",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^1.0.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-config": "^4.0.0",
    "webpack-dev-server": "^1.14.1"
